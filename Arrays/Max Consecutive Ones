class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        int maxi = Integer.MIN_VALUE;
        int cnt = 0;
        for(int i = 0;i<nums.length;i++){
            if(nums[i]==1){
                cnt++;
            }
            else{
               
                cnt = 0;
            }
             maxi = Math.max(cnt,maxi);
        }
        return maxi;
    }
}
